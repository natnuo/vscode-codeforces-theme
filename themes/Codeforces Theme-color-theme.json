{
	"name": "Codeforces Theme",
	"colors": {
		"editor.background": "#fff",
		"editor.foreground": "#000",
		"list.activeSelectionIconForeground": "#fff",
		"sideBar.background": "#fff",
		"activityBar.background": "#fff",
		"activityBar.foreground": "#000",
		"tab.inactiveBackground": "#fff",
		"tab.activeBorderTop": "#800",
		"commandCenter.background": "#fff",
		"statusBar.background": "#fff",
		"menu.background": "#fff",
		"statusBar.noFolderBackground": "#fff",
		"statusBar.foreground": "#000",
		"menubar.selectionBackground": "#fff",
		"titleBar.activeBackground": "#fff",
		"titleBar.inactiveBackground": "#fff"
	},
	"tokenColors": [
		{
			"name": "Comments and Preprocessor Tags",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"keyword.control.directive"
			],
			"settings": {
				"foreground": "#800",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#660"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#008",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#606",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#333"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#066"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#080",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Qualified Types",
			"scope": "meta.template.call",
			"settings": {
				"foreground": "#080",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Booleans",
			"scope": [
				"constant.language.true.cpp",
				"constant.language.false.cpp"
			],
			"settings": {
				"foreground": "#008",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Included Packages",
			"scope": "string.quoted.other.lt-gt.include.cpp",
			"settings": {
				"foreground": "#000",
				"fontStyle": ""
			}
		},
		{
			"name": "Namespace Directive",
			"scope": "storage.type.namespace.directive.cpp",
			"settings": {
				"foreground": "#008",
				"fontStyle": "bold"
			}
		}
	]
}
